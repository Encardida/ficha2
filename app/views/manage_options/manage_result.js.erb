
$("#spinner").fadeOut(1500, function () {
  $("#alert-partial").hide().html("<%= escape_javascript(render("manage_result")) %>").slideDown(400, function() {
    setTimeout(function(){
      $('#alert-partial').html("<%= escape_javascript(render('manage_result')) %>").slideUp(300);
    }, 2000);
  });
  $("#partial-manage-options").hide().html("<%= escape_javascript(render("manage_options")) %>").fadeIn(1000);
});
