<%- model_class = Ficha -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
  <%= javascript_include_tag "pagination.js" %>

<div id="fichas">
  <%= render 'fichas' %>
</div>

<%= will_paginate @fichas%>

<div class="pagination">
  <div class="pagination">

    <a class="previous_page" rel="prev" href="/fichas?page=3">
      <span class="translation_missing" title="translation missing: pt-BR.will_paginate.previous_label">Previous Label</span>
    </a>
    <a href="/fichas?page=1">1</a>
    <a href="/fichas?page=2">2</a>
    <a rel="prev" href="/fichas?page=3">3</a>
    <em class="current">4</em>
    <a rel="next" href="/fichas?page=5">5</a>
    <a class="next_page" rel="next" href="/fichas?page=5">
      <span class="translation_missing" title="translation missing: pt-BR.will_paginate.next_label">Next Label</span>
    </a>
  </div>
</div>

<% # puts render_pages(Ficha.all, params[:page]) %>


<nav aria-label="Page navigation">
  <ul class="pagination">
    <li class="disabled">
      <a href="#" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
      </a>
    </li>
    <li class="active"><a href="/fichas?page=1">1</a></li>
    <li><a href="/fichas?page=2">2</a></li>
    <li><a href="/fichas?page=3">3</a></li>
    <li><a href="/fichas?page=4">4</a></li>
    <li><a href="/fichas?page=5">5</a></li>
    <li>
      <a href="#" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
      </a>
    </li>
  </ul>
</nav>
