
<div class="page-header">
  <h1><%= t('users') %></h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th>id</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>
